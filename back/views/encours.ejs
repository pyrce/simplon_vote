<!DOCTYPE html>
<html lang="en">

<head>
  <% include partials/head %>
</head>

<body class="container">

  <header>
    <% include partials/header %>
  </header>

  <main>
    <div class="row">

      <div class="col-md-12">
   <% votes.forEach(element =>  { %>
        <div class="card">
          <div class="card-body">
            <p class="text-center"><b><%= element.subject %></b> <br /> <small>By <%= element.createdBy.login %></small></p>
            <p class="text-right"> <%= element.nbVote %>/ <%= element.quota %> nombre de vote ▶️</p>
            <div class="progress">
              <div class="progress-bar bg-warning" role="progressbar" style="width: 50%;" aria-valuenow="50"
                aria-valuemin="0" aria-valuemax="100">50%</div>
            </div>
          </div>
        </div><br />

      </div>
    
 

    


    <% });  %>

 
    </div>
  </main>

  <footer>
    <% include partials/footer %>
  </footer>

  <% include partials/modals %>


</body>

</html>